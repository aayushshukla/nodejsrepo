<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Employee Portal</title>
</head>
<body>
     <%- include('partials/header') %>
    <h1> Employees Details </h1>
    <table border="1">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email </th>
            <th>Update </th>
            <th>Delete </th>
        </tr>
      
    <% 
       allemployeesDetails.forEach(emp => { %>
        <tr>
        
        <td><%= emp.fist_name %>   </td>
        <td><%= emp.last_name %>   </td>
        <td><%= emp.email %>   </td>
        <td><a href="">Update</a>  </td>
        <td><a href="http://localhost:8000/web/employees/delete?eid=<%=emp._id %>"> Delete </td>
        </tr>
     <%  });
    %>
  </table>



    <%- include("partials/footer") %>
</body>
</html>